model Medicines {
  id        String @id @default(uuid())
  seller_id String
  user      User    @relation(fields: [seller_id], references: [id], onDelete: Cascade)
  categories_id String?
  name      String
  image     String
  stock     Int
  price     Int
  description String
  reviews Reviews[]

  @@index([seller_id])
  @@map("medicines")
}
