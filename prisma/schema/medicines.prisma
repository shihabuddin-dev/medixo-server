model Medicines {
  id        String @id @default(uuid())
  sellerId String
  user      User    @relation(fields: [sellerId], references: [id], onDelete: Cascade)
  categories_id String?
  name      String
  image     String
  stock     Int
  price     Int
  description String
  reviews Reviews[]

  @@index([sellerId])
  @@map("medicines")
}
