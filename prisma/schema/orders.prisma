model Orders {
    id               String   @id @default(uuid())
    customerId      String
    user             User     @relation(fields: [customerId], references: [id], onDelete: Cascade)
    status           String
    shipping_address String
    created_at       DateTime @default(now())
    quantity         Int
    price            Int

    @@index([customerId])
    @@map("orders")
}